<configs>
	<append xpath="/blocks">

		
		<block name="GrassBlock">
			<property name="Material" value="Mdirt"/>
			
			<property name="Shape" value="ModelEntity"/>
			<property name="Path" value="solid"/>
			<property name="Model" value="#@modfolder:Resources/GB.unity3d?GrassBlock.prefab"/><property name="ModelOffset" value="0,0,0"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="OnlySimpleRotations" value="true"/>
			<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>	
			<property name="CustomIcon" value="gb1" />
			<property name="Stabilitysupport" value="false"/>
			<property name="MaxDamage" value="500"/>
			<property name="CreativeMode" value="Player"/>
			<property name="Stacknumber" value="250"/>
			<property class="RepairItems"> <property name="resourceClayLump" value="10"/> </property>
			
